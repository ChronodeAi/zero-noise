# Zero Noise - Environment Configuration
# Copy to .env and fill in your values

# Filebase IPFS RPC API
FILEBASE_IPFS_RPC_ENDPOINT=https://rpc.filebase.io
FILEBASE_IPFS_RPC_KEY=your_base64_key_here

# Storacha (Web3.Storage)
STORACHA_DID=did:key:z6Mk...your_did_here

# Optional: Filebase S3 API (for S3-compatible uploads)
FILEBASE_S3_ENDPOINT=https://s3.filebase.com
FILEBASE_S3_ACCESS_KEY=your_access_key
FILEBASE_S3_SECRET_KEY=your_secret_key
